{
  "name": "@kallinen/openapi-axios-client",
  "version": "0.5.0",
  "description": "A lightweight TypeScript library to generate fully typed API clients from OpenAPI specifications.",
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --config ./jest.config.js --colors --verbose --forceExit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kallinen/openapi-axios-client.git"
  },
  "author": "Kallinen Solutions Oy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kallinen/openapi-axios-client/issues"
  },
  "homepage": "https://github.com/kallinen/openapi-axios-client#readme",
  "dependencies": {
    "axios": "^1.11.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/jest": "^29.4.4",
    "axios-mock-adapter": "^2.1.0",
    "cross-env": "^7.0.3",
    "jest": "^29.0.3",
    "jest-mock-server": "^0.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.9.2"
  },
  "directories": {
    "lib": "lib"
  }
}
