{
  "name": "@kallinen/openapi-axios-client",
  "version": "0.4.0",
  "description": "A lightweight TypeScript library to generate fully typed API clients from OpenAPI specifications.",
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --config ./jest.config.js --colors --verbose --forceExit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kallinen/openapi-axios-client.git"
  },
  "author": "Softphant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/softphant/axios-wrapper/issues"
  },
  "homepage": "https://github.com/softphant/axios-wrapper#readme",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^14.2.0",
    "@seriousme/openapi-schema-validator": "^2.5.0",
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-mock-server": "^0.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "directories": {
    "lib": "lib"
  }
}
